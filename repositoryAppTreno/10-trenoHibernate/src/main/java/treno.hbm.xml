<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.corso.hibernate.treno.entities.TrenoDTO" table="treni">
		<id name="id">
			<generator class="native"></generator>
		</id>

		<property name="sigla" column="sigla" not-null="true"></property>
		<property name="factory" column="factory" not-null="false"></property>

	</class>

</hibernate-mapping> 